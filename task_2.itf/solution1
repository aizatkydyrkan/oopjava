package interfaces1;

public class solution1 {
    public interface CanFly {
        void fly();
    }

    public interface CanMove {
        void move();
    }

    public interface CanEat {
        void eat();
    }

    public static void main(String[] args) {
        Duck duck = new Duck();
        duck.move();
        duck.eat();
        duck.fly();
        Dog dog = new Dog();
        dog.move();
        dog.eat();
        Car car = new Car();
        car.move();
        Airplane airplane = new Airplane();
        airplane.fly();
    }

}
